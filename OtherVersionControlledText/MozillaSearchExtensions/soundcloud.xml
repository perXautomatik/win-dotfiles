<SearchPlugin xmlns="http://www.mozilla.org/2006/browser/search/" xmlns:os="http://a9.com/-/spec/opensearch/1.1/">
<os:ShortName>SoundCloud</os:ShortName>
<os:Description>Search for tracks and users on SoundCloud</os:Description>
<os:InputEncoding>UTF-8</os:InputEncoding>
<os:Image width="16" height="16">data:image/x-icon;base64,</os:Image>
<SearchForm>http://soundcloud.com/search</SearchForm>
<os:Url type="text/html" method="GET" template="http://soundcloud.com/search?q={searchTerms}">
</os:Url><os:Url type="application/x-suggestions+json" method="GET" template="http://soundcloud.com/search/suggestions.js?q={searchTerms}">
</os:Url>
</SearchPlugin>