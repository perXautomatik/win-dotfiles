[plugins]
[profiles]
  [[default]]
    background_darkness = 0.85
    background_type = transparent
    cursor_color = "#ffffff"
    custom_command = pwd; cd "$(cat /tmp/last_used_pwd.txt)"; pwd; cd -; sudo -E su; pwd; cd "$(cat /tmp/last_used_cd.txt)"; pwd; cd "$(cat /tmp/last_used_pwd.txt)"; pwd; cd -; bash; pwd; cd "$(cat /tmp/last_used_cd.txt)"; pwd; cd "$(cat /tmp/last_used_pwd.txt)"; cd -;
    exit_action = restart
    font = Monospace 17
    foreground_color = "#ffffff"
    login_shell = True
    scrollback_lines = 10000000
    show_titlebar = False
    split_to_group = True
    use_custom_command = True
    use_system_font = False
  [[Debian7Profile]]
    background_darkness = 0.85
    background_type = transparent
    cursor_color = "#ffffff"
    custom_command = '''sshlogin="root@10.3.50.180"; counter=10; while [[ $counter -gt 0 ]]; do counter=$((counter-1)); sleeptime=$(( RANDOM % 60 + 1 )); ssh "$sshlogin"; printf '%s - Reconnecting ssh "%s" in "%s" seconds...\n' "$(date)" "$sshlogin" "$sleeptime"; sleep "$sleeptime"; done; bash;'''
    exit_action = restart
    font = Monospace 17
    foreground_color = "#ffffff"
    login_shell = True
    scrollback_lines = 10000000
    show_titlebar = False
    split_to_group = True
    use_custom_command = True
    use_system_font = False
