{
    "type": "script",
    "description": "Force Chrome Ctrl+Shift+C",
    "store": {},
    "modes": [
        3
    ],
    "usageCount": 1,
    "prompt": false,
    "omitTrigger": false,
    "showInTrayMenu": false,
    "abbreviation": {
        "abbreviations": [],
        "backspace": true,
        "ignoreCase": false,
        "immediate": false,
        "triggerInside": false,
        "wordChars": "[\\w]"
    },
    "hotkey": {
        "modifiers": [
            "<ctrl>",
            "<shift>"
        ],
        "hotKey": "c"
    },
    "filter": {
        "regex": "chromium-browser.Chromium-browser",
        "isRecursive": false
    }
}